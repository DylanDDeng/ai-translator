{
  "version": 2,
  "regions": ["hkg1"],
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Connection",
          "value": "keep-alive"
        }
      ]
    }
  ]
}
