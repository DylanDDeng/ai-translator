{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "regions": ["hkg1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Connection",
          "value": "keep-alive"
        },
        {
          "key": "Keep-Alive",
          "value": "timeout=60"
        }
      ]
    }
  ]
}
